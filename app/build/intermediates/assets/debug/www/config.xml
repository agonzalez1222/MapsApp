<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bright.moments.parkit" version="1.0.0" xmlns="http://www.w3.org/ns/widgets"
xmlns:cdv="http://cordova.apache.org/ns/1.0">
 <name>Park It</name>
 <description>
 A simple, location aware app to help you find your car
 </description>
 <author email="agonzalez9@ggc.edu">
 </author>
 <content src="index.html" />
 <access origin="*" />
 <allow-intent href="http://*/*" />
 <allow-intent href="https://*/*" />
 <allow-intent href="tel:*" />
 <allow-intent href="sms:*" />
 <allow-intent href="mailto:*" />
 <allow-intent href="geo:*" />
 <platform name="android">
 <allow-intent href="market:*" />
 </platform>
 <platform name="ios">
 <allow-intent href="itms:*" />
 <allow-intent href="itms-apps:*" />
 </platform>
 <preference name="Orientation" value="portrait" />
 <preference name="target-device" value="handset" />
 <preference name="deployment-target" value="7.0" />
 <preference name="SuppressLongPressGesture" value="true" />
 <preference name="Suppress3DTouchGesture" value="true" />
 <preference name="InAppBrowserStorageEnabled" value="true" />
 <engine name="android" spec="^7.0.0" />
 <engine name="ios" spec="^4.4.0" />
 <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
 <engine name="browser" spec="~5.0.3" />
 <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
 <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
 <plugin name="cordova-plugin-statusbar" spec="^2.4.1" />
</widget>
